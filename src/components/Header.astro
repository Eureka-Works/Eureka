<style>
  @import url("https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap")
  @font-face {
    font-family: Eureka;
    src: url(/BroncoPersonalUse.ttf);
  }

  @font-face {
    font-family: Barett;
    src: url(/Barett Street.ttf);
  }

  header {
    display: flex;
    justify-content: space-between;
    background-color: #E1DCCF;
    padding: 10px;
    margin: 10px;
    border-radius: 20px;
    box-shadow: 1px 9px 13px -2px rgba(0, 0, 0, 0.48);
    -webkit-box-shadow: 1px 9px 13px -2px rgba(0, 0, 0, 0.48);
    -moz-box-shadow: 1px 9px 13px -2px rgba(0, 0, 0, 0.48);
  }

  h1 {
    color: black;
    font-family: Barett;
    font-size: 3rem;
    padding: 10px;
  }

  ul {
    list-style-type: none;
    color: black;
    display: flex;
    gap: 2rem;
    justify-items: center;
    align-items: center;
    font-size: 2rem;
    font-family: "Roboto", sans-serif;
    font-weight: 600;
  }

  .Logo {
    display: flex;
    justify-content: center;
  }

  img {
    height: 6rem;
  }

  .icons {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 10px;
  }
</style>
